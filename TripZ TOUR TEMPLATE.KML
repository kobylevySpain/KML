<Document>
	<name>TripZ.travel TOUR TEMPLATE - TOUR #XXXXXXXX - NAME OF TOUR</name>
	<StyleMap id="simple">
		<Pair>
			<key>normal</key>
			<styleUrl>#simple_n</styleUrl>
		</Pair>
		<Pair>
			<key>highlight</key>
			<styleUrl>#simple_h</styleUrl>
		</Pair>
	</StyleMap>
	<Style id="simple_n">
		<IconStyle>
			<scale>1.1</scale>
			<Icon>
				<href>http://maps.google.com/mapfiles/kml/pushpin/ylw-pushpin.png</href>
			</Icon>
			<hotSpot x="20" y="2" xunits="pixels" yunits="pixels"/>
		</IconStyle>
	</Style>
	<Style id="simple_h">
		<IconStyle>
			<scale>1.3</scale>
			<Icon>
				<href>http://maps.google.com/mapfiles/kml/pushpin/ylw-pushpin.png</href>
			</Icon>
			<hotSpot x="20" y="2" xunits="pixels" yunits="pixels"/>
		</IconStyle>
	</Style>

<!--   **********PLACEMARK 1***********************   -->

	<Placemark id="1">
		<name>Tour Eiffel</name>
		<description>Lovely monument</description>
		<LookAt>
			<longitude>2.294318</longitude>
			<latitude>48.8583179</latitude>
			<altitude>150</altitude>
			<heading>75</heading>
			<tilt>25</tilt>
			<range>800</range>
			<gx:altitudeMode>relativeToSeaFloor</gx:altitudeMode>
		</LookAt>
		<styleUrl>#simple</styleUrl>
		<Point>
			<gx:drawOrder>1</gx:drawOrder>
			<coordinates>2.294318,348.8583179,0</coordinates>
		</Point>
	</Placemark>
<!--   **********END of PLACEMARK 1***********************   -->

<!--   **********PLACEMARK 2***********************   -->

